{
    "name": "rafael_dx/facturama-php-sdk",
    "description": "Facturama PHP SDK",
    "keywords": ["API", "PHP", "JSON", "Facturama"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://apisandbox.facturama.mx/",
    "support": {
        "issues": "https://github.com/Facturama/facturama-php/issues"
    },
    "authors": [
        {
            "name": "Facturama",
            "email": "dev@facturama.com"
        }
    ],
    "require": {
        "php": "^5.6|>=7.2|8.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.4.3|^6.0|^7.0",
        "phpmd/phpmd": "^2.4.3"
    },
    "autoload": {
        "psr-4": {
            "Facturama\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": { "Facturama\\Tests\\": "tests/" },
        "files": [ "examples/credentials.php" ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
